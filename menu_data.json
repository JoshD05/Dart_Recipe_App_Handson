        { "stepOrder": 4, "instruction": "Add curry paste and cook for 1 minute, stirring." },
        { "stepOrder": 5, "instruction": "Add mixed vegetables and stir to coat." },
        { "stepOrder": 6, "instruction": "Pour in coconut milk and bring to a simmer." },
