      "name": "Spaghetti Bolognese",
      "ingredients": [
        { "name": "Ground Beef", "quantity": 500, "unitOfMeasure": "g" },
        { "name": "Canned Tomatoes", "quantity": 400, "unitOfMeasure": "g" },
        { "name": "Onion", "quantity": 1, "unitOfMeasure": "unit" },
        { "name": "Garlic", "quantity": 2, "unitOfMeasure": "cloves" },
        { "name": "Spaghetti", "quantity": 300, "unitOfMeasure": "g" },
