  {
    "dayOfWeek": "Sunday",
    "recipe": {
      "name": "Roast Chicken Dinner",
      "ingredients": [
        { "name": "Whole Chicken", "quantity": 1.5, "unitOfMeasure": "kg" },
        { "name": "Potatoes", "quantity": 800, "unitOfMeasure": "g" },
        { "name": "Carrots", "quantity": 4, "unitOfMeasure": "units" },
        { "name": "Onions", "quantity": 2, "unitOfMeasure": "units" },
        { "name": "Rosemary", "quantity": 2, "unitOfMeasure": "sprigs" },
        { "name": "Olive Oil", "quantity": 3, "unitOfMeasure": "tbsp" },
        { "name": "Salt", "quantity": 1, "unitOfMeasure": "tsp" },
        { "name": "Black Pepper", "quantity": 0.5, "unitOfMeasure": "tsp" }
      ],
      "steps": [
        { "stepOrder": 1, "instruction": "Preheat oven to 200°C (400°F)." },
        { "stepOrder": 2, "instruction": "Pat chicken dry with paper towels. Rub with olive oil, salt, pepper, and rosemary." },
        { "stepOrder": 3, "instruction": "Chop potatoes, carrots, and onions into large chunks." },
        { "stepOrder": 4, "instruction": "Place chicken in a roasting pan. Arrange vegetables around the chicken." },
        { "stepOrder": 5, "instruction": "Roast for 1 hour 15 minutes to 1 hour 45 minutes, or until internal temperature of chicken reaches 74°C (165°F) and vegetables are tender and browned." },
        { "stepOrder": 6, "instruction": "Let chicken rest for 10 minutes before carving." },
        { "stepOrder": 7, "instruction": "Serve chicken with roasted vegetables." }
      ]
    }
  }
