        { "name": "Parmesan Cheese", "quantity": 50, "unitOfMeasure": "g" }
      ],
      "steps": [
        { "stepOrder": 1, "instruction": "Heat olive oil in a large pot." },
        { "stepOrder": 2, "instruction": "Add chopped onion and garlic, cook until softened." },
        { "stepOrder": 3, "instruction": "Add ground beef and cook until browned, breaking it up with a spoon." },
