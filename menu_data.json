  {
    "dayOfWeek": "Thursday",
    "recipe": {
      "name": "Fish and Chips",
      "ingredients": [
        { "name": "Cod Fillets", "quantity": 400, "unitOfMeasure": "g" },
        { "name": "Potatoes", "quantity": 500, "unitOfMeasure": "g" },
        { "name": "All-purpose Flour", "quantity": 100, "unitOfMeasure": "g" },
        { "name": "Beer (or sparkling water)", "quantity": 150, "unitOfMeasure": "ml" },
        { "name": "Salt", "quantity": 1, "unitOfMeasure": "tsp" },
        { "name": "Vegetable Oil", "quantity": 500, "unitOfMeasure": "ml" }
      ],
      "steps": [
        { "stepOrder": 1, "instruction": "Peel and cut potatoes into thick chips. Rinse and pat dry." },
        { "stepOrder": 2, "instruction": "Heat vegetable oil in a deep pan to 170°C (340°F)." },
        { "stepOrder": 3, "instruction": "Fry chips in batches until golden and crisp. Drain and season with salt." },
        { "stepOrder": 4, "instruction": "For the batter, whisk flour, salt, and beer until smooth." },
        { "stepOrder": 5, "instruction": "Dip fish fillets into the batter, ensuring they are fully coated." },
        { "stepOrder": 6, "instruction": "Carefully lower battered fish into the hot oil. Fry for 5-7 minutes until golden brown and cooked through." },
        { "stepOrder": 7, "instruction": "Drain on paper towels." },
        { "stepOrder": 8, "instruction": "Serve fish immediately with the chips." }
      ]
    }
  },
  {
