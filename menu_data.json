    "recipe": {
      "name": "Homemade Pizza",
      "ingredients": [
        { "name": "Pizza Dough", "quantity": 1, "unitOfMeasure": "ball" },
        { "name": "Tomato Sauce", "quantity": 100, "unitOfMeasure": "ml" },
        { "name": "Mozzarella Cheese", "quantity": 150, "unitOfMeasure": "g" },
        { "name": "Pepperoni", "quantity": 50, "unitOfMeasure": "g" },
        { "name": "Mushrooms", "quantity": 100, "unitOfMeasure": "g" },
        { "name": "Olive Oil", "quantity": 1, "unitOfMeasure": "tbsp" }
      ],
      "steps": [
        { "stepOrder": 1, "instruction": "Preheat oven to 220°C (425°F) with a pizza stone or baking steel if you have one." },
        { "stepOrder": 2, "instruction": "Roll out pizza dough on a floured surface to your desired thickness." },
        { "stepOrder": 3, "instruction": "Transfer dough to a parchment-lined baking sheet or pizza peel." },
        { "stepOrder": 4, "instruction": "Spread tomato sauce evenly over the dough, leaving a small border for the crust." },
        { "stepOrder": 5, "instruction": "Sprinkle with mozzarella cheese, then add pepperoni and sliced mushrooms." },
        { "stepOrder": 6, "instruction": "Drizzle with a little olive oil." },
        { "stepOrder": 7, "instruction": "Bake for 10-15 minutes, or until the crust is golden and the cheese is bubbly and slightly browned." },
        { "stepOrder": 8, "instruction": "Slice and serve hot." }
      ]
    }
  },
